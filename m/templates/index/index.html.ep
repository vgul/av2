            var prepared0 = jQuery.map( regions0, function(v,i) {
                return( '<li>' + v + '</li>' );  
            });
            var str0 = "<ul>" + prepared0.join('') + "</ul>";
            // console.log( 'Here: ' + str0 );
            jQuery( "#" + mode_id ).click( function() {
                jQuery("#regions").html(str0);
            });
        } else {
            // console.log( 'Abnormal' );
            jQuery( "#" + mode_id ).click( function() {
                jQuery("#regions").html( 'fff' );
            });
        } 

