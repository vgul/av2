
<div class="row row-centered">
<div class="col-md-12 ">

    <h4><b>Тел</b>: <%= $phones_to_page %></h4>

<table class="table table-condensed table-condensed-small-bottom">
<% my $old_hier = ''; %>
<% foreach my $a ( @{ $retro } ) { %>
<tr>
<td> <%== $a->[0] %> </td>
<td>
    <% if( $old_hier ne $a->[2] ) { %>
        <b><%= $a->[2] %></b><br />
    <% } %>
    <%= $a->[1] %> 
</td>
</tr>
    <% $old_hier = $a->[2]; %>
<% } %>
</table>

</table>
</div><!-- col-md-12 -->
</div><!-- row -->

